    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
